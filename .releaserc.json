{
  "branches": ["main", "master"],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    ["@semantic-release/npm", { "npmPublish": false, "pkgRoot": "menubar-tts" }],
    "@semantic-release/github",
    ["@semantic-release/exec", { "publishCmd": "npm --prefix menubar-tts run release" }]
  ]
}
